<% title @member.full_name %>

    <%= render :partial => "projects_nav" %>

    <!--/nav_wrap-->

    <div id="stream_filters" class="form-inline">
        <span class="filters all">
            <label for="filter__">
            <%= radio_button_tag(:filter, "*", :checked => 'checked') %> All
            </label>
        </span>
        <span class="filters art_filter">
            <label for="filter_.art_fil">
            <%= radio_button_tag(:filter, ".art_fil") %> Arts
            </label>
        </span>
        <span class="filters biz_filter">
            <label for="filter_.biz_fil">
            <%= radio_button_tag(:filter, ".biz_fil") %> Entrepreneurial
            </label>
        </span>
        <span class="filters music_filter">
            <label for="filter_.music_fil">
            <%= radio_button_tag(:filter, ".music_fil") %> Music
            </label>
        </span>
        <span class="filters sports_filter">
            <label for="filter_.sports_fil">
            <%= radio_button_tag(:filter, ".sports_fil") %> Sports & Rec
            </label>
        </span>
        <span class="filters other_filter">
            <label for="filter_.other_fil">
            <%= radio_button_tag(:filter, ".other_fil") %> Other
            </label>
        </span>
    </div>

    <div id="prs_wrap">
        <% if @projects.count > 0 %>
            <div id="media_query_prev">
                <div id="prof_prs">
                    <div id="prs">
                        <%= render :partial => "projects_fav" %>
                    </div>
                </div>         
            </div>
            <% else %>
                <div class="none_message">
                    No Favorites Yet
                </div>
        <% end %>

        <%= will_paginate @projects, class: 'display_none' %>

        <% if @projects.total_pages != @projects.current_page %>
            <div id="loading"></div>
            <%= link_to("#", class: "com_show_wrap load_arrow") do %>
                <div class="com_show">
                    Show More
                </div>
            <% end %>
        <% end %>
    </div>