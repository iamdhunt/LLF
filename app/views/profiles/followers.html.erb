<% title @member.full_name + " | Followers" %>

<div id="follow_title">
	<div class="label-info">
		Followers
	</div>
</div>

<div id="follow_list_wrap">
	 <% if @followers.count > 0 %>
		<%= render :partial => "followers" %>
	<% else %>
	    <div class="none_message">
	        No Followers Yet
	    </div>
	<% end %>   

</div>