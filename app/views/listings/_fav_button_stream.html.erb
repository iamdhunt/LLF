<% if listing.votes.size > 0 && current_member.voted_up_on?(listing) %>
  <%= link_to image_tag("Favorite.png", class: "ims-act", title: "Unfavorite", alt: "Favorite"), favorite_listing_path(listing), remote: true, method: :put %>
<% else %>
  <%= link_to image_tag("Favorite 2.png", class: "ims-act", title: "Favorite", alt: "Favorite"), favorite_listing_path(listing), remote: true, method: :put %>
<% end %>