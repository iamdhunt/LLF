<% if @listing.votes.size > 0 && current_member.voted_up_on?(@listing) %>
  <%= link_to image_tag("Favorite 8.png", class: "ims fav", title: "Unfavorite", alt: "Favorite"), favorite_listing_path(@listing), method: :put, :remote => true %>
<% else %>
  <%= link_to image_tag("Favorite 7.png", class: "ims fav", title: "Favorite", alt: "Favorite"), favorite_listing_path(@listing), method: :put, :remote => true %>
<% end %>