<% if event.votes.size > 0 && current_member.voted_up_on?(event) %>
  <%= link_to image_tag("Favorite.png", class: "ims-act", title: "Unfavorite", alt: "Favorite"), favorite_event_path(event), method: :put %>
<% else %>
  <%= link_to image_tag("Favorite 2.png", class: "ims-act", title: "Favorite", alt: "Favorite"), favorite_event_path(event), method: :put %>
<% end %>