<% if @medium.votes.size > 0 && current_member.voted_up_on?(@medium) %>
  <%= link_to image_tag("Favorite.png", class: "ims fav", title: "Unfavorite", alt: "Favorite"), favorite_medium_path(@medium), method: :put, :remote => true %>
<% else %>
  <%= link_to image_tag("Favorite 6.png", class: "ims fav", title: "Favorite", alt: "Favorite"), favorite_medium_path(@medium), method: :put, :remote => true %>
<% end %>
